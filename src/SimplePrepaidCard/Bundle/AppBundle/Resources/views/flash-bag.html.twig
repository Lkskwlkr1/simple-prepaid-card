{% for type, messages in app.session.flashBag.all %}
    {% for message in messages %}
        {%if type == 'success'%} {% set type = 'success' %} {%endif%}
        <div class="alert alert-{{ type }}">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
            {{ message|raw }}
        </div>
    {% endfor %}
{% endfor %}